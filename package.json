    "@rowanmanning/eslint-config": "^3.3.0",
    "chai": "^4.3.6",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "testdouble": "^3.16.4"
  },
  "main": "lib/varname.js",
  "nyc": {
    "eager": true,
    "reporter": [
